{
  "name": "Stamp",
  "description": "Stamp helps you manage your entire Accounts Payable inside of Gmail.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["https://*.trystamp.ai/*", "https://*.ngrok.app/*", "https://*.ngrok-free.app/*", "https://70h4jbuv95.execute-api.us-east-2.amazonaws.com/*", "http://localhost:*/*", "https://localhost:*/*"],
      "js": ["oauth-callback-detector.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "identity",
    "storage",
    "scripting",
    "tabs",
    "activeTab",
    "webRequest"
  ],
  "host_permissions": [
    "https://mail.google.com/",
    "https://*.your-backend.com/",
    "https://*.trystamp.ai/*",
    "https://*.ngrok.app/*", 
    "https://*.ngrok-free.app/*",
    "https://*.execute-api.us-east-2.amazonaws.com/*",
    "https://70h4jbuv95.execute-api.us-east-2.amazonaws.com/*",
    "https://nmuo25f2da.execute-api.us-east-2.amazonaws.com/*",
    "http://localhost:*/*",
    "https://localhost:*/*"
  ],
  "oauth2": {
    "client_id": "759225635526-gs69pgupgap87o4ul9ud9pv8pjrupcgc.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'none'"
  },
  "web_accessible_resources": [
    {
      "matches": ["https://mail.google.com/*"],
      "resources": [
        "pageWorld.js",
        "pageWorld.js.map",
        "stamp-logo.png",
        "google-logo.png",
        "jspreadsheet.css",
        "jsuites.css",
        "stamp-spreadsheet-theme.css",
        "floating-chat/*",
        "*.map"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Stamp Accounts Payable"
  },
  "key": ""
}